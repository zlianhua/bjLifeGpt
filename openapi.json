{
    "openapi": "3.1.0",
    "info": {
      "title": "Database Query",
      "description": "Query your database in English.",
      "version": "v0.0.1"
    },
    "servers": [{ "url": "https://bj-life-gpt.vercel.app" }],
    "paths": {
      "/query": {
        "post": {
          "description": "Executes a supabase query to retrieve data.",
          "operationId": "query",
          "parameters": [],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/queryModel"
                }
              }
            },
            "required": true
          },
          "deprecated": false,
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "result": {
                        "description": "The query result"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/insert": {
        "post": {
          "description": "Executes a supabase insert data.",
          "operationId": "insert",
          "parameters": [],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/insertModel"
                }
              }
            },
            "required": true
          },
          "deprecated": false,
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "result": {
                        "description": "The insert result"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "queryModel": {
          "type": "object",
          "properties": {
            "table": {
              "type": "string",
              "description": "The name of the table to query."
            },
            "select": {
              "type": "string",
              "description": "The selection string specifying the fields to return."
            },
            "conditions": {
              "type": "object",
              "additionalProperties": {
                "type": "string"
              },
              "description": "A dictionary of conditions to filter the query."
            }
          },
          "required": ["table", "select", "conditions"],
          "description": "A model for querying data from a database table with specific conditions."
        },
        "insertModel": {
          "type": "object",
          "properties": {
            "table": {
              "type": "string",
              "description": "The name of the table where data will be inserted."
            },
            "data": {
              "type": "object",
              "additionalProperties": true,
              "description": "The data to be inserted into the table. This is a dictionary where each key-value pair represents a column name and its value to be inserted."
            }
          },
          "required": ["table", "data"],
          "description": "A model for inserting data into a database table."
        }
      }  
    }
}
